package model;

import java.util.HashMap;
import java.util.List;

import android.graphics.Bitmap;

public class Product {
	private HashMap<String,String> specs;
	private String id;
	private String name;
	private int price;
	private int stock;
	List<Bitmap> images;
	
	public Product(String id)
	{
		this.id = id;
	}
	
	public HashMap<String,String> getspecs()
	{
		return specs;
	}
	
	public void setspecs(HashMap<String,String> attr)
	{
		this.specs = attr;
	}
	
	public List<Bitmap> getImages()
	{
		return images;
	}
	
	public void setImages(List<Bitmap> img)
	{
		images = img;
	}
	
	
}
